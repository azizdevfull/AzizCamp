<div class="container">
  <h1 class="text-center">Moderator</h1>
  <%= render 'moderator/nav_links' %>
  <table class="table table-dark table-striped">
      <thead>
      <tr>
        <th scope="col">Id</th>
        <th scope="col">Username</th>
        <th scope="col">Email</th>
        <th scope="col">Password</th>
      </tr>
    </thead>
    <tbody>
      <% @users.each do |user| %>
        <tr>
          <th scope="row"><%= user.id %></th>
          <td><%= user.username  %></td>
          <td><%= user.email %></td>
          <td> <%= user.password %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>